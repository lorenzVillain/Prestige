<div class="container">
  <div style="margin-top: 75px;">
    <%= form_for @product do |f| %>
      <%= f.label :title %>
      <%= f.select :title, Product::CATEGORIES %>
      <%= f.label :description %>
      <%= f.text_field :description %>
      <%= f.label  :brand %>
      <%= f.text_field :brand %>
      <%= f.label  :price %>
      <%= f.text_field :price %>


     <%= f.button "Create" %>

    <% end %>
  </div>
</div>

