JEWELRY

   <!-- Main jumbotron for a primary marketing message or call to action -->
   <div class="jumbotron">
     <div class="container">
       <h1>JEWELRY</h1>
       <p>SHINE, SHINE SHINE! </p>
       <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more &raquo;</a></p>
     </div>
   </div>


<div class="row">
<% @products.each do |product| %>
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <%= image_tag product.picture_path %>
      <div class="caption">
        <h3><%= product.title %></h3>
        <p><%= product.description %></p>
        <p><%=product.brand %></p>
        <p><%=product.price %></p>
        <p>
          <%= link_to "SHOW", product_path(product), class: "btn btn-primary" %>
          <a href="#" class="btn btn-default" role="button">ORDER</a></p>

      </div>
    </div>
  </div>
<% end %>
</div>


